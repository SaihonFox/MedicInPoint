<ResourceDictionary xmlns="https://github.com/avaloniaui"
					xmlns:sys="using:System"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<FontFamily x:Key="Nunito">Fonts#Nunito</FontFamily>

	<!-- Button -->
	<SolidColorBrush x:Key="ButtonBackground" Color="#33000000" />
	<SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="#FF000000" Opacity="0.1" />
	<SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#66000000" />
	<SolidColorBrush x:Key="ButtonBackgroundDisabled" Color="#33000000" />
	<CornerRadius x:Key="ButtonCornerRadius">6</CornerRadius>
	<Thickness x:Key="ButtonPadding">8,6</Thickness>
	<!-- Button -->

	<!-- Window -->
	<SolidColorBrush x:Key="WindowBackground" Color="#FAFAFA" />
	<!-- Window -->
</ResourceDictionary>