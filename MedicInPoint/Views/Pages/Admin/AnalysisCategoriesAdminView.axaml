<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:MedicInPoint.ViewModels.Pages.Admin"
			 xmlns:dialogHost="clr-namespace:DialogHostAvalonia;assembly=DialogHost.Avalonia"
			 mc:Ignorable="d"
			 d:DesignWidth="1500" d:DesignHeight="1000"
			 x:Class="MedicInPoint.Views.Pages.Admin.AnalysisCategoriesAdminView"
			 x:DataType="vm:AnalysisCategoriesAdminViewModel">
	<Design.DataContext>
		<vm:AnalysisCategoriesAdminViewModel />
	</Design.DataContext>

	<Panel>
		<Grid>
			<Button Classes="back" Command="{CompiledBinding Back}" Margin="30,50,0,0" HorizontalAlignment="Left" />
			<TextBox Classes="search" Watermark="Искать категорию" Margin="0,40,0,0" VerticalAlignment="Top" Width="500" Height="50" />

			
		</Grid>
	</Panel>
</UserControl>