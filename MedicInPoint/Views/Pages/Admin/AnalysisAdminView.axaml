<UserControl xmlns="https://github.com/avaloniaui"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:drawer="using:MedicInPoint.Views.UserControls.Drawers"
			 xmlns:vm="using:MedicInPoint.ViewModels.Pages.Admin"
			 xmlns:dialog="using:MedicInPoint.Views.Pages"
			 xmlns:i="clr-namespace:Avalonia.Xaml.Interactivity;assembly=Avalonia.Xaml.Interactivity"
			 xmlns:bh="using:TestAutoCompleteBehavior.Behaviors"
			 mc:Ignorable="d"
			 d:DesignWidth="1500" d:DesignHeight="1000"
			 x:Class="MedicInPoint.Views.Pages.Admin.AnalysisAdminView"
			 x:DataType="vm:AnalysisAdminViewModel">
	<Design.DataContext>
		<vm:AnalysisAdminViewModel />
	</Design.DataContext>

	<Panel>
		<Grid>
			<Button Classes="back" Command="{CompiledBinding Back}" Margin="30,50,0,0" HorizontalAlignment="Left" />
			<AutoCompleteBox Name="acb" Classes="search" IsDropDownOpen="False" Watermark="Искать анализ" FilterMode="ContainsOrdinal" Margin="0,40,0,0" HorizontalAlignment="Center" VerticalAlignment="Top" Width="500" Height="50">
				<i:Interaction.Behaviors>
					<bh:AutoCompleteZeroMinimumPrefixLengthDropdownBehavior />
				</i:Interaction.Behaviors>
			</AutoCompleteBox>
			<!-- <TextBox Classes="search" Watermark="Искать анализ" Margin="0,40,0,0" VerticalAlignment="Top" Width="500" Height="50" /> -->

			<Grid ColumnDefinitions="*,auto" Margin="30,125">
				<Grid Grid.Column="0" RowDefinitions="auto,*">
				</Grid>

				<drawer:AnalysisDrawerView Grid.Column="1" />
			</Grid>
		</Grid>
	</Panel>
</UserControl>